!!! 5
%html
  %head
    %title Inform
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyDPE4JHp_V_12lo7-21kmlmlOGZUt9050c&sensor=true"
    = javascript_include_tag "application"
    = csrf_meta_tags

    %meta{ name: 'viewport', content: 'width=device-width; initial-scale=1.0; maximum-scale=1.0;' }
  %body
    .navbar.navbar-fixed-top
      .navbar-inner
        .container
          = link_to 'Inform', root_path, class: 'brand'

          %ul.nav
            %li= link_to 'Home', root_path
            %li= link_to 'Sites', sites_path
            %li= link_to 'Organization', organizations_path

    .container
      = yield
